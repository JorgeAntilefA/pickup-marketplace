{
  "expo": {
    "name": "Dafiti Pickup",
    "slug": "MarketplacePickup",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ic_launcher.png",
    "updates": {
      "enabled": false
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dafiti.pickup"
    },
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "AUDIO_RECORDING",
        "CAMERA",
        "CAMERA_ROLL"
      ],
      "package": "com.dafiti.pickup",
      "versionCode": 1
    }
  }
}
